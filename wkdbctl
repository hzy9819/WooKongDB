		RUN_TPCC_TEST "focc" $1 $2
		# RESTART_CLUSTER
		sleep 10
		CHECK_LTS_EXIST
		RUN_TPCC_TEST "bocc" $1 $2
		# RESTART_CLUSTER
		sleep 10
		CHECK_LTS_EXIST
		RUN_TPCC_TEST "dta" $1 $2
		# RESTART_CLUSTER
		sleep 10
		set -e
		MV_SEG_LOG occtpcc_${dirtimestamp}
		DRAW_TEST_PICTURE3 "tpcc" focc bocc dta
	fi
}

