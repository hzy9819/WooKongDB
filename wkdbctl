set key box
set output
" >> draw-for-$1-test-network-receive-for-$2-$3-$4.plt
	gnuplot draw-for-$1-test-network-receive-for-$2-$3-$4.plt
	echo -n "set term pngcairo lw 2
set xlabel \"number of thread\"
