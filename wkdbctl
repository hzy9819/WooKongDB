		DRAW_TEST_PICTURE3 "tpcb" focc bocc dta
	fi

	if [[ "$TEST_BENCHMARK" ]]
	then
		set +e
		CHECK_LTS_EXIST
		RUN_BENCHMARKOCC_TPCC_TEST "focc"
		sleep 10
		CHECK_LTS_EXIST
		RUN_BENCHMARKOCC_TPCC_TEST	"bocc"
		sleep 10
		CHECK_LTS_EXIST
		RUN_BENCHMARK_TPCC_TEST	"dta"
		sleep 10
		set -e		
	fi

	if [[ "$TEST_TPCC" ]]
